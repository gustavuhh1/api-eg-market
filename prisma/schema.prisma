generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DB_URL")
  directUrl = env("BD_DIRECT_URL")
  schemas   = ["master"]
}

model products {
  id       Int  @id @default(autoincrement())
  name     String
  price    Float
  img_link String?
  category String

  @@schema("master")
}
